{
  "name": "@se-2/subgraph",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "graph build",
    "clean-node": "rm -rf graph-node/data/",
    "codegen": "graph codegen",
    "create-local": "graph create --node http://localhost:8020/ scaffold-eth/betflix",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ betflix",
    "deploy-sepolia": "graph deploy --studio betflix",
    "graph": "graph",
    "sepolia-ship": "yarn codegen && yarn build --network sepolia && yarn deploy-sepolia",
    "run-node": "cd graph-node && docker compose up",
    "stop-node": "cd graph-node && docker compose down",
    "test": "graph test -d"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.97.0",
    "@graphprotocol/graph-ts": "0.38.0",
    "ts-node": "10.9.2",
    "typescript": "5.7.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "^20.11.17",
    "matchstick-as": "~0.6.0"
  }
}
